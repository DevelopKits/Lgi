			MemDumpViewer.o \
			SpaceTabConv.o \
			GDocView.o \
			GMdi.o \
			LgiMain.o \
			Png.o \
			FindSymbol.o

# Target
# Executable target
$(Target) : ../$(BuildDir)/liblgi$(Tag).so outputfolder $(Depends)
	@echo Linking $(Target) [$(Gui)/$(Build)]...
	$(CC) -rdynamic -Wl,-soname,$(TargetFile),-export-dynamic,-R. -o \
		$(Target) $(addprefix $(BuildDir)/,$(Depends)) $(Libs)
	@echo Done.

../$(BuildDir)/liblgi$(Tag).so : FORCE
	export Build=$(Build); \
		export Gui=$(Gui); \
		$(MAKE) -C .. -f Makefile.linux

FORCE :

# Create the output folder
outputfolder :
	-mkdir -p $(BuildDir) 2> /dev/null

# Clean out targets
clean :
	rm -f $(BuildDir)/*.o $(Target).
	@echo Cleaned $(BuildDir)

