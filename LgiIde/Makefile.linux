.SILENT :

CC = g++
Target = lgiide
ifndef Build
	Build = Debug
endif
Gui = X
BuildDir = $(Build)$(Gui)

Flags = -fPIC -w -fno-inline -rdynamic -fpermissive
Defs = -DLINUX -D_REENTRANT -D$(Gui)WIN -DGTK

ifeq ($(Build),Debug)
	Defs += -D_DEBUG
	Flags += -g
	Tag = d
else
	Flags += -s -Os
endif

# Libraries
Libs = 		-L../$(BuildDir) \
			-llgi$(Tag)

# Includes
Inc =		-I../include/common \
			-I../include/beos \
			-I./Code

# Dependencies
Depends =	FtpThread.o \
			IFtp.o \
			GLexCpp.o \
			SysCharSupport.o \
			FindInFiles.o \
			GHistory.o \
			IdeDoc.o \
